#include <iostream>
using namespace std;
int main() {

	string shopping[] = { "bread", "corn", "apples", "potato", "strawberries", " "};

	shopping[5] = "milk";

	for (int i = 0; i < 6; i++)
		cout << shopping[i] << " ";
	cout << endl;

	shopping[0] = "potato";

	for (int i = 0; i < 6; i++)
		cout << shopping[i] << " ";
	cout << endl;

	cout << shopping[2];

	cout << endl;

	for (int i = 0; i < 5; i++)
		if (shopping[i] == "strawberries")
			cout << "found strawberrioes" << endl;
	cout << endl;
}
